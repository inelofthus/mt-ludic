<?xml version="1.0" encoding="UTF-8"?>
<runtime:Runtime
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:runtime="platform:/plugin/no.hal.pg.runtime/model/runtime.ecore"
	xmlns:exampletask-runtime="platform:/plugin/no.hal.pg.exampletask.runtime/model/exampletask.ecore"
    xsi:schemaLocation="platform:/plugin/no.hal.pg.runtime/model/runtime.ecore ../../../../../../../no.hal.pg.runtime/model/runtime.ecore platform:/plugin/no.hal.pg.exampletask.runtime/model/runtime.ecore ../../../../../../../no.hal.pg.exampletask.runtime/model/exampletask.ecore">
  <games>
    <tasks xsi:type="exampletask-runtime:ExampleTask" correctAnswer="true"/>
    <tasks>
      <startConditions
          xsi:type="runtime:IsTaskStartedCondition"
          context="//@games.0/@tasks.0"/>
    </tasks>
    <tasks>
      <startConditions
          xsi:type="runtime:IsTaskFinishedCondition"
          context="//@games.0/@tasks.0"/>
    </tasks>
    <tasks>
      <startConditions
          xsi:type="runtime:CompositeCondition">
        <conditions
            xsi:type="runtime:IsTaskStartedCondition"
            context="//@games.0/@tasks.0"/>
        <conditions
            xsi:type="runtime:IsTaskFinishedCondition"
            context="//@games.0/@tasks.0"/>
      </startConditions>
    </tasks>
    <tasks>
      <startConditions
          xsi:type="runtime:CompositeCondition"
          logic="true">
        <conditions
            xsi:type="runtime:IsTaskStartedCondition"
            context="//@games.0/@tasks.0"/>
        <conditions
            xsi:type="runtime:IsTaskFinishedCondition"
            context="//@games.0/@tasks.0"/>
      </startConditions>
    </tasks>
  </games>
</runtime:Runtime>
