<?xml version="1.0" encoding="UTF-8"?>
<ineruntime:LeaderboardGame xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:ineruntime="platform:/plugin/no.inela.runtime/model/runtime.ecore" xmlns:puzzletask="platform:/plugin/no.inela.puzzletask.runtime/model/puzzletask.ecore">
  <players nickname="Ine">
    <person href="../Person1.xmi#/"/>
  </players>
  <players nickname="Mari">
    <person href="../Person2.xmi#/"/>
  </players>
  <players nickname="Wenche">
    <person href="../Person3.xmi#/"/>
  </players>
  <tasks xsi:type="puzzletask:PuzzleTask">
    <puzzles xsi:type="puzzletask:SimplePuzzle" level="1" solution="fruit">
      <puzzlePieces image="apple" playerCount="1"/>
      <puzzlePieces image="orange"/>
      <puzzlePieces image="banana" playerCount="1"/>
      <playerPieces key="//@players.0" value="//@tasks.0/@puzzles.0/@puzzlePieces.2"/>
      <playerPieces key="//@players.1" value="//@tasks.0/@puzzles.0/@puzzlePieces.0"/>
    </puzzles>
    <puzzles xsi:type="puzzletask:SimplePuzzle" level="2" solution="organ">
      <puzzlePieces image="pipe organ"/>
      <puzzlePieces image="heart"/>
      <puzzlePieces image="brain"/>
    </puzzles>
    <playerLevels key="//@players.0" value="1"/>
    <playerLevels key="//@players.1" value="1"/>
    <playerTaskScores/>
  </tasks>
  <tasks/>
  <leaderboard>
    <playerTotalScores key="//@players.0" value="150"/>
    <playerTotalScores key="//@players.1" value="30"/>
    <playerTaskScores task="//@tasks.0">
      <playerScores key="//@players.0" value="100"/>
    </playerTaskScores>
    <playerTaskScores task="//@tasks.1">
      <playerScores key="//@players.0" value="50"/>
      <playerScores key="//@players.1" value="30"/>
    </playerTaskScores>
  </leaderboard>
</ineruntime:LeaderboardGame>
