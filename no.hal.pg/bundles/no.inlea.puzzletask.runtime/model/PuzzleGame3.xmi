<?xml version="1.0" encoding="UTF-8"?>
<runtime:Game
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:puzzletask="platform:/plugin/no.inela.puzzletask.runtime/model/puzzletask.ecore"
    xmlns:runtime="platform:/plugin/no.hal.pg.runtime/model/runtime.ecore"
    xsi:schemaLocation="platform:/plugin/no.inela.puzzletask.runtime/model/puzzletask.ecore puzzletask.ecore platform:/plugin/no.hal.pg.runtime/model/runtime.ecore ../../no.hal.pg.runtime/model/runtime.ecore">
  <players nickname="Ine">
    <person
        href="Person1.xmi#/"/>
  </players>
  <players nickname="mari">
    <person
        href="Person2.xmi#/"/>
  </players>
  <tasks xsi:type="puzzletask:PuzzleTask"
      players="//@players.0">
    <puzzles
        xsi:type="puzzletask:SimplePuzzle"
        level="1"
        solution="fruit">
      <puzzlePieces
          players="//@players.1"
          image="apple"/>
      <puzzlePieces
          players="//@players.1"
          image="banana"/>
      <puzzlePieces
          image="orange"/>
    </puzzles>
    <puzzles
        xsi:type="puzzletask:SimplePuzzle"
        level="1"
        solution="organ">
      <puzzlePieces
          image="heart"/>
      <puzzlePieces
          image="brain"/>
      <puzzlePieces
          image="pipe organ"/>
    </puzzles>
    <playerLevels
        key="//@players.0"
        value="1"/>
    <playerLevels
        key="//@players.0"
        value="1"/>
  </tasks>
</runtime:Game>
